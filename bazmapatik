// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;

int bajanarar(int a, int b){
    while(b != 0){
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
int main() {
    // Write C++ code here
    int a,b;
    cout << "a = ";
    cin >> a;
    cout << "b = ";
    cin >> b;
    
    int c = (a * b) / bajanarar(a,b);
    cout << "amenapoqr bazmapatik " << c;
    return 0;
}
